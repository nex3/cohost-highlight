{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowJs": true,
    "rootDir": ".",
    "useUnknownInCatchVariables": false,
    "declaration": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2019"],
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2017"
  },
  "include": ["**/*.ts"],
  "exclude": ["**/*.test.ts"]
}
